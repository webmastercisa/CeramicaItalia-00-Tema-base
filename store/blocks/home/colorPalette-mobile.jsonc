"slider-layout#infocards-colores": {
    "title": "infocard colores",
    "props": {
        "itemsPerPage": {
            "desktop": 7,
            "tablet": 5,
            "phone": 4
        },
        "infinite": true,
        "showNavigationArrows": "never",
        "showPaginationDots": "never",
        // "centerMode": "to-the-left",
        "blockClass": ["slider-infocards-colors", "slider-infocards-colors-1"],
        "arrowSize": 10,
        "navigationStep":1,
        "autoplay": {
          "timeout": 3000,
          "stopOnHover": true
        }
        //   "blockClass": [ "btns"]
    }
},
