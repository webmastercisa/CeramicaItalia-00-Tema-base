{

    "flex-layout.row#container-tab-home": {
      "children": ["tab-layout#tab-vitrina"],
      "title": "tab-vitrinas-colores",
      "props": {
        "blockClass": ["container-tab-tab-vitrina-"]
      }
    },
    "tab-layout#tab-vitrina":{ 
    "children": [
      "tab-list#tab-vitrina",
      "tab-content#tab-vitrina"
    ],
    "props": {
      "blockClass": "tab-vitrina",
      "defaultActiveTabId": "tab-vitrina-1"
    }
  },
  "tab-list#tab-vitrina": {
    "children": [
        "tab-list.item.children#tab1",
        "tab-list.item.children#tab2",
        "tab-list.item.children#tab3",
        "tab-list.item.children#tab4"
    ],
    "props": {
        "blockClass": "tab-list-vitrina"
    }
  },
  "tab-list.item.children#tab1": {
    "title": "title-tab-1",
    "props": {
      "tabId": "tab-vitrina-1",
      "defaultActiveTab": true,
      "blockClass": "tab-item-children"
    },
    "children": [
        "flex-layout.row#tab1-name"
    ]
  },
  "tab-list.item.children#tab2": {
    "title": "title-tab-2",
    "props": {
      "tabId": "tab-vitrina-2",
      "blockClass": "tab-item-children"
