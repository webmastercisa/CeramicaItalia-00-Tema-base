        "flex-layout.row#tab2-name"
    ]
  },

  "tab-list.item.children#tab3": {
    "title": "title-tab-3",
    "props": {
      "tabId": "tab-vitrina-3",
      "blockClass": "tab-item-children"
    },
    "children": [
        "flex-layout.row#tab3-name"
    ]
  },

  "tab-list.item.children#tab4": {
    "title": "title-tab-4",
    "props": {
      "tabId": "tab-vitrina-4",
      "blockClass": "tab-item-children"
    },
    "children": [
        "flex-layout.row#tab4-name"
    ]
  },


  
 "tab-content#tab-vitrina": {
   "children": [
     "tab-content.item#tab-vitrina-1",
     "tab-content.item#tab-vitrina-2",
     "tab-content.item#tab-vitrina-3",
     "tab-content.item#tab-vitrina-4"
   ]
 },
 "tab-content.item#tab-vitrina-1": {
  "title": "content-tab-color-1",
   "children": [
    "flex-layout.row#miniShelf-col",
