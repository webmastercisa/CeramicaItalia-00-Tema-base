//  tab1 
"flex-layout.row#tab1-name": {
  "title": "titulo-vitrina-color1",
  "children": ["image#tab1-name","rich-text#tab1-name"],
  "props": {
    "blockClass": ["tab1-name"],
    "preventHorizontalStretch": true,
    "preserveLayoutOnMobile": true
  }
},

"image#tab1-name": {
    "props": {
             "src": "assets/ico/home/color1.svg",
             "alt": "alt",
             "height": "30px",
             "blockClass": "tab1-name"
    }
},

"rich-text#tab1-name": {
    "props": {
             "text": "Color1",
             "blockClass": "tab1-name",
             "textAlignment": "CENTER",
             "textPosition": "CENTER"
    }
},

