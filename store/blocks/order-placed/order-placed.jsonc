            "flex-layout.col#order-placed-col2-mobile",
            "flex-layout.col#order-placed-col-mobile"
        ],
        "props": {
            "blockClass": [
                "orderplaced-desktop",
                "orderplaced-mobile"
            ]
        }
    },
    "flex-layout.col#order-placed-col-mobile": {
