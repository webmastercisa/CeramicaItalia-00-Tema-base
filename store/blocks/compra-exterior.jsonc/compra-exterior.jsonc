    },
    "rich-text#title-search-compra-exterior": {
      "props": {
           "text": "Tu nuevo hogar podria necesitar...",
           "blockClass": "title-search-muestras",
           "textAlignment": "CENTER",
           "textPosition": "CENTER"
      }
    },


    "image#linea-search-compra-exterior": {
