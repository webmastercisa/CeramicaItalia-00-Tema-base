          "children": [
            "image#banner1-compras-exterior",
            "image#banner2-compras-exterior",
            "image#banner3-compras-exterior",
            "image#banner4-compras-exterior"
          ],
          "props": {
            "blockClass": ["container-banners-compras-exterior"]
          }
        },

        "image#banner1-compras-exterior": {
            "props": {
                     "src": "arquivos/banner-comprar-exterior.png",
                     "alt": "alt",
                     "height": "hight",
                     "blockClass": "banner1-compra-exterior",
                     "link": {
                             "url": "#",
                             "openNewTab": false,
                             "title": "title"
                }
            }
        },

        "image#banner2-compras-exterior": {
