    "flex-layout.row#title-search-compra-exterior": {
      "children": ["flex-layout.row#container-title-search-compra-exterior", "image#linea-search-compra-exterior"],
      "props": {
        "blockClass": ["title-search-muestras" , "title-search-compra-exterior"],
