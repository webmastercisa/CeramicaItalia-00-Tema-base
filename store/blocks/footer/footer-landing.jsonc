{
    "footer#templateLanding": {
        "blocks": [
            "footer-layout.desktop#templateLanding",
            "footer-layout.mobile#templateLanding"
        ]
    },
    "footer-layout.desktop#templateLanding": {
        "children": [
            "flex-layout.row#contact-info-templateLanding"
        ]
    },
    "footer-layout.mobile#templateLanding": {
        "children": [
            "flex-layout.row#contact-info-templateLanding-mobile"
        ]
    },
    "flex-layout.row#contact-info-templateLanding": {
        "children": [
            // "flex-layout.row#how-to-buy-footer",
            "modal-trigger#how-to-buy-footer",
            "flex-layout.row#whatsapp-footer",
            "flex-layout.row#delivery-times-footer",
            "flex-layout.row#payment-methods-footer"
        ],
        "props": {
            "blockClass": [
                "contact-footer",
                "bloquePrincipal"
            ],
            "fulwidth": true,
            "preventHorizontalStretch": true,
            "preventVerticalStretch": true,
            "preserveLayoutOnMobile": true,
            "horizontalAlign": "center",
            "verticalAlign": "middle",
            "paddingTop": 0,
            "paddingBottom": 0
        }
    },
    // Sección ¿Cómo Comprar? footer
    "flex-layout.row#how-to-buy-footer": {
        "children": [
            "image#how-to-buy-footer",
            "rich-text#how-to-bye-text"
        ],
        "props": {
            "blockClass": "how-to-buy-footer",
            "fulwidth": true,
            "preventHorizontalStretch": true,
            "preventVerticalStretch": true,
            "preserveLayoutOnMobile": true,
            "horizontalAlign": "center",
            "verticalAlign": "middle",
            "paddingTop": 0,
            "paddingBottom": 0
        }
    },
    "image#how-to-buy-footer": {
        "props": {
            "src": "assets/ico/howToByIconStroke.svg",
            "alt": "¿Cómo comprar?",
            "height": "20px",
            "blockClass": "how-to-buy-footer-img"
            // "link": {
            //     "url": "/",
            //     "openNewTab": true,
            //     "title": "¿Cómo comprar?"
            // }
        }
    },
    "rich-text#how-to-bye-text": {
        "props": {
            "text": "¿Cómo comprar?",
            "blockClass": "how-to-buy-footer-text",
            "textAlignment": "CENTER",
            "textPosition": "CENTER"
        }
    },

    // Sección Whatsapp footer
    "flex-layout.row#whatsapp-footer": {
        "children": [
            "image#whatsapp-img-footer",
            "rich-text#whatsapp-text-footer"
