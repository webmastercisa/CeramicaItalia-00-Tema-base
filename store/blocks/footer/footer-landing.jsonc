    "image#whatsapp-img-footer": {
        "props": {
            "src": "assets/ico/whatsapp-icon-footer.svg",
            "alt": "WhatsApp",
            "height": "20px",
            "blockClass": "contact-info-img",
            "link": {
                "url": "https://api.whatsapp.com/send/?phone=5715141167&text&type=phone_number&app_absent=0",
                "openNewTab": true,
                "title": "WhatsApp"
            }
        }
    },
    "rich-text#whatsapp-text-footer": {
        "props": {
            "text": "[Escríbenos](https://wa.me/5715141167?target=_blank)",
            "blockClass": "whatsapp-text-footer",
            "textAlignment": "CENTER",
            "textPosition": "CENTER"
