{
  "disclosure-layout-group#header": {
    "children": [
      "disclosure-layout#header1",
      "disclosure-layout#header2",
      "disclosure-layout#header3"
    ]
  },
  // section pisos
  "disclosure-layout#header1": {
    "children": [
      "disclosure-trigger#header1",
      "disclosure-content#header1"
    ],
    "props": {}
  },
  "disclosure-trigger#header1": {
    "children": [
      "flex-layout.col#menu-iconPisos"
    ],
    "props": {
      "blockClass": [
        "trigger-header1"
      ]
    }
  },
