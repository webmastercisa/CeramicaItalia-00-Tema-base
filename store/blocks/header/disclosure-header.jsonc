    ],
    "props": {
      "infoCards": [
        {
          "id": "infocard-pisos1",
          "title": "infocard-pisos1",
          "isFullModeStyle": false,
          "textPosition": "left",
          "imageUrl": "https://ceramicaitalia.vteximg.com.br/arquivos/banner200x200.png",
          "imageActionUrl": "/interiores",
          // "headline": "Interiores",
          "callToActionText": "Interiores",
          "blockClass": [
            "infocard-pisos"
          ]
        },
        {
          "id": "infocard-pisos1",
